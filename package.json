{
  "name": "gitflex",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Flex your GitHub profile",
  "keywords": [
    "github",
    "profile",
    "showcase",
    "developer",
    "portfolio",
    "neomorphic",
    "react",
    "tailwind"
  ],
  "author": "Nishad Kindre <nishadkindre@gmail.com>",
  "license": "MIT",
  "homepage": "https://gitflexx.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitflex/gitflex.git"
  },
  "bugs": {
    "url": "https://github.com/gitflex/gitflex/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "axios": "^1.12.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-github-calendar": "^4.5.10",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.0",
    "simple-icons": "^15.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
